//for clsx XSSFworkbook=new XSSFworkbook(fis);
HSSFWorkbook wb=new HSSFWorkbook(fis);
HSSFSheet sheet1=wb.getSheetAt(0);
String data1=sheet1.getRow(0).getCell(0).getStringCellValue();
}
